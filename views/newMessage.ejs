<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <%-include('partials/header')%>
        <form action="">
            <p>
                <label for="title">Title</label>
                <input type="text" id="title" name="title" required >
            </p>

            <p>
                <label for="text">Message</label>
                <textarea 
                    id="text" 
                    name="text"
                    cols="33" 
                    placeholder="Write your message here ..." >
                </textarea>
            </p>

            <button type="submit">Submit</button>
        </form>
    </body>
</html>